
"prefs
set nu
set encoding=utf8
let $LANG='en'
set langmenu=en
syntax on
set incsearch
set smarttab
set history=1000

set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx


"theme
colo desert
set termguicolors
let g:solarized_extra_hi_groups=1
let g:airline_solarized_bg='dark'
let g:airline_theme='solarized'

